<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anjass.raihan.monica20">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Fragment.ItemListFragment"></activity>
        <activity android:name=".Fragment.TodoListFragment" />
        <activity android:name=".MainFragmentActivity" />
        <activity
            android:name=".FragmentMainActivity"
            android:label="@string/title_activity_fragment_main" />
        <activity
            android:name=".HomeActivity$FragmentMainActivity"
            android:label="@string/title_activity_fragment_main" />
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Home.HomeScreen"
            android:label="@string/title_activity_home_screen"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>